}

function newgame()
{
document.getElementById("0_1").value = "   ";
document.getElementById("0_2").value = "   ";
document.getElementById("0_0").value = "   ";
document.getElementById("1_1").value = "   ";
document.getElementById("1_2").value = "   ";
document.getElementById("1_0").value = "   ";
document.getElementById("2_0").value = "   ";
document.getElementById("2_2").value = "   ";
document.getElementById("2_1").value = "   ";
}

