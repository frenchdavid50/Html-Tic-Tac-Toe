<html>
<Head>
<Title?Tic Tac Toe</Title>

<SCRIPT TYPE="TEXT/JAVASCRIPT">

function squareclicked(square)
// squareclicked is a function that is called whenever a button is clicked.
{
 var value = square.value;
 if (value != 'X' && value != 'O') {
   square.value = 'X';
   makeComputerMove();
 }
}

function makeComputerMove()
{
val11 = document.getElementById("1_1").value;
val01 = document.getElementById("0_1").value;
val02 = document.getElementById("0_2").value;
val10 = document.getElementById("1_0").value;
val20 = document.getElementById("2_0").value;
val11 = document.getElementById("1_1").value;
val12 = document.getElementById("1_2").value;
val21 = document.getElementById("2_1").value;
val22 = document.getElementById("2_2").value;
val00 = document.getElementById("0_0").value;

if (val00 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val10 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val20 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val01 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val02 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val22 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val12 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
} else if (val21 == 'X' && val11 != 'X' && val11 != 'O') {
  document.getElementById("1_1").value = "O";
 } 
 
}
 
  
  

 
 

 
</SCRIPT>

</head>

<body>

<INPUT TYPE="BUTTON" ID="0_0" VALUE="   "ONCLICK="squareclicked(this);">
<INPUT TYPE="BUTTON" ID="1_0" VALUE="   "ONCLICK="squareclicked(this);">
<INPUT TYPE="BUTTON" ID="2_0" VALUE="   "ONCLICK="squareclicked(this);">
<BR>
<INPUT TYPE="BUTTON" ID="0_1" VALUE="   "ONCLICK="squareclicked(this);">
<INPUT TYPE="BUTTON" ID="1_1" VALUE="   "ONCLICK="squareclicked(this);">
<INPUT TYPE="BUTTON" ID="2_1" VALUE="   "ONCLICK="squareclicked(this);">
